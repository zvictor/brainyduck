{
  "name": "brainyduck",
  "version": "0.0.56",
  "description": "Quickly build powerful backends using only your graphql schemas",
  "repository": "github:zvictor/brainyduck",
  "keywords": [
    "BaaS",
    "backend",
    "fauna",
    "graphql",
    "serverless",
    "low-code",
    "lowcode"
  ],
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "bin": "./cli.js",
  "exports": {
    ".": {
      "types": "./.cache/sdk.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./cache": "./locateCache.cjs",
    "./bundlers/*": "./bundlers/*.cjs",
    "./utils": "./utils.js",
    "./*": "./commands/*.js"
  },
  "main": "./index.cjs",
  "types": "./.cache/sdk.d.ts",
  "files": [
    "/bundlers",
    "/commands",
    "/scripts",
    "*.cjs",
    "*.js",
    "tsconfig.json",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "rm -Rf .cache",
    "test": "./tests/run-tests.sh"
  },
  "dependencies": {
    "@graphql-codegen/core": "2.6.0",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-graphql-request": "4.5.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@graphql-tools/load": "7.7.0",
    "@graphql-tools/merge": "8.3.0",
    "@graphql-tools/url-loader": "7.12.1",
    "@types/node": "18.0.4",
    "@types/react": "18.0.15",
    "chalk": "^4.1.2",
    "chokidar": "3.5.3",
    "commander": "9.4.0",
    "constant-case": "3.0.4",
    "debug": "4.3.4",
    "execa": "6.1.0",
    "fauna-shell": "0.15.0",
    "faunadb": "4.6.0",
    "figures": "4.0.1",
    "globby": "13.1.2",
    "gql-generator": "1.0.17",
    "graphql": "16.5.0",
    "graphql-request": "4.3.0",
    "graphql-tag": "2.12.6",
    "log-symbols": "5.1.0",
    "ora": "6.1.2",
    "p-queue": "7.2.0",
    "resolve-as-bin": "2.1.0",
    "tempy": "3.0.0",
    "tsup": "6.1.3",
    "typescript": "4.7.4"
  },
  "optionalDependencies": {
    "node-fetch": "^2.6.7"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/zvictor"
  }
}
